#event
scoreboard players add @s AIBC_event 0
execute if score @s AIBC_event matches ..-1 run function phan:editor/panel/ai_waypoint/event_text/_index
execute if score @s AIBC_event matches 1.. run function phan:editor/panel/ai_waypoint/event_text/_index
tellraw @s ["",{text:"[<] ",color:"yellow",bold:false,click_event:{action:"run_command",command:"/trigger tEditor set 1050"}},{text:"[>] ",color:"yellow",bold:false,click_event:{action:"run_command",command:"/trigger tEditor set 1051"}},{text:"AIBC_event = "},{score:{name:"@s",objective:"AIBC_event"}}]
execute unless entity @s[scores={AIBC_event=0,AIBC_modifier=0}] run tellraw @s[scores={AIBC_modifier=-2147483648..2147483647}] ["",{translate:"gp.editor.button.set",color:"gold",bold:false,click_event:{action:"suggest_command",command:"/scoreboard players set @s AIBC_modifier "}},{text:" [-] ",color:"red",bold:false,click_event:{action:"run_command",command:"/trigger tEditor set 1052"}},{text:"[+] ",color:"green",bold:false,click_event:{action:"run_command",command:"/trigger tEditor set 1053"}},{text:"AIBC_modifier = "},{score:{name:"@s",objective:"AIBC_modifier"}}]
execute if entity @s[tag=!eventBadSkill,tag=!eventHighSkill] unless score @s AIBC_event matches 0 run tellraw @s ["",{translate:"gp.editor.button.high_skill",color:"gray",bold:false,click_event:{action:"run_command",command:"/trigger tEditor set 1054"}},{text:" "},{translate:"gp.editor.button.low_skill",color:"gray",bold:false,click_event:{action:"run_command",command:"/trigger tEditor set 1055"}}]
