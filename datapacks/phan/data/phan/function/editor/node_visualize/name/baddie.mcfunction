#show name
summon area_effect_cloud ~ ~ ~ {Particle:{type:"block",block_state:{Name:"air"}},WaitTime:0,Duration:5,CustomNameVisible:1b,CustomName:["",{translate:"gp.editor.node_type.baddie",color:"dark_red"}]}

